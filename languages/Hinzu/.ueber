[
  % Abstract syntax of Hinzu
  macro(eslSyntax(hinzu)),

  % Dump Hinzu metadata
  function(dump, [], [hinzu(term)], hinzuDump:main, []),
  elementOf('dump.hinzu', hinzu(term)),
  mapsTo(dump, [], ['dump.hinzu']),

  % Publish Hinzu in Markdown
  function(languageToMd, [hinzu(term)], [markdown(text), markdown(text)], hinzuLanguageToMd:main, []),
  function(mdIndex, [hinzu(term)], [markdown(text)], hinzuMdIndex:main, []),
  elementOf('../../docs/index.md', markdown(text)),
  mapsTo(mdIndex, ['dump.hinzu'], ['../../docs/index.md']),
  function(mdFiles, [], [], hinzuMdFiles:main, []),
  mapsTo(mdFiles, [], [])
].
